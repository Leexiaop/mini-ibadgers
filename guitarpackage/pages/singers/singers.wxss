/* guitarpackage/pages/singers/singers.wxss */
.singers {
	min-height: 90vh;
	box-sizing: border-box;
	padding-bottom: 32rpx;
	display: flex;
	flex-direction: column;
	justify-content: space-between;
}
.singers > .content {
	flex: 1;
}
.singers > .empty {
	width: 100%;
	flex: 1;
	display: flex;
	align-items: center;
	justify-content: center;
}
.group {
	display: flex;
	flex-wrap: wrap;
	justify-content: space-between;
}