/**javascript.wxss**/
page {
    background-color: var(--td-bg-color-container) !important;
  }
  
  page .round-image {
    border-radius: 12rpx;
  }
  
  .side-bar-wrapper {
    display: flex;
    height: 100vh;
  }
  
  .side-bar-wrapper .content {
    flex: 1;
  }
  
  .side-bar-wrapper .section {
    padding: 32rpx 0;
  }
  
  .side-bar-wrapper .title {
    padding-left: 40rpx;
    margin-bottom: 8rpx;
    line-height: 52rpx;
    color: var(--td-text-color-primary);
  }
  
  .side-bar-wrapper .image {
    width: 96rpx;
    height: 96rpx;
  }
  
.group {
	width: 100%;
	box-sizing: border-box;
}
.group > .grid {
	width: 686rpx;
	margin: 24rpx auto;
}
.group > .group-item {
	width: 686rpx;
	margin: 0 auto;
}
.group > .group-item > .tip {
	display: flex;
	justify-content: space-between;
	flex-wrap: wrap;
}
